#include<stdio.h>
#include<math.h>
int factorial(int n)
{
    if(n==0||n==1)
    {
        return 1;
    }
    else
    {
        return n*factorial(n-1);
    }
}
int main()
{
    int n,fact=1;
    printf("enter the number");
    scanf("%d",&n);
    int res=factorial(n);
    printf("%d",res);
    return 0;
}
